{
  "name": "analytics-backend",
  "version": "1.0.0",
  "description": "Self-hosted analytics backend using ClickHouse",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "pack-script": "sh -c 'if [ public/script-full.js -nt public/script.js ]; then terser --compress --mangle < public/script-full.js | tr -d \"\\n\" > public/script.js; else echo \"script.js up to date\"; fi'",
    "dev": "tsc && node dist/index.js",
    "build": "tsc && npm run pack-script",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:check": "drizzle-kit check --config=drizzle.config.ts"
  },
  "dependencies": {
    "@better-auth/stripe": "1.2.7",
    "@clickhouse/client": "1.11.1",
    "@fastify/cors": "11.0.1",
    "@fastify/one-line-logger": "2.0.2",
    "@fastify/static": "8.1.1",
    "@maxmind/geoip2-node": "6.1.0",
    "better-auth": "1.2.7",
    "dotenv": "16.5.0",
    "drizzle-orm": "0.43.1",
    "fastify": "5.3.3",
    "fastify-better-auth": "1.0.4",
    "isbot": "^5.1.28",
    "luxon": "3.6.1",
    "node-cache": "^5.1.2",
    "node-cron": "4.0.6",
    "pg": "8.16.0",
    "postgres": "3.4.5",
    "psl": "^1.15.0",
    "resend": "^4.5.1",
    "sqlstring": "2.3.3",
    "stripe": "18.1.0",
    "ua-parser-js": "2.0.3",
    "undici": "7.9.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@types/luxon": "3.6.2",
    "@types/node": "20.14.8",
    "@types/pg": "8.15.1",
    "@types/psl": "^1.1.3",
    "@types/sqlstring": "2.3.2",
    "drizzle-kit": "0.31.1",
    "terser": "^5.40.0",
    "ts-node-dev": "2.0.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vitest": "^3.2.2"
  }
}
